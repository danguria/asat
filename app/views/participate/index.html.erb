<script>
    document.getElementById("judge-nav").className = "active";
</script>

<% @contest_info.each do |contest, ps| %>
<br>
  <h2><%= contest %> </h2>
  <table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th>Division</th>
        <th>Round</th>
        <th>View Contestant</th>
      </tr>
    </thead>
    
    <tbody>
      <% ps.each do |p| %>
        <tr>
          <td><%= p[:division] %></td>
          <td><%= p[:round] %></td>
          <td> <%= link_to 'See Contestant', participate_path(:id => p[:id]) %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>